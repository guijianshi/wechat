<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.qiuzhizhushou.wechat.mapper.TagMapper">

    <select id="getHotTag" resultType="cn.qiuzhizhushou.wechat.model.Tag">
        select *
        from tag
        where id in (2, 15, 53, 64, 69, 74)
    </select>
</mapper>